version: '3.8'

services:
  hdhomerun_proxy:
    image: hdhomerun_proxy
    container_name: hdhomerun_app_proxy
    restart: on-failure
    ports:
      - "65001:65001"
    environment:
      - TUNER_PROXY_IP=192.168.100.25
      - DEBUG = True
    command: ["python", "hdhomerun_tuner_proxy.py", "$${TUNER_PROXY_IP}"]
    